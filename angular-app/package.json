    "optimize-css-assets-webpack-plugin": "~5.0",
    "sass-loader": "~7.1",
    "style-loader": "~0.23",
    "to-string-loader": "~1.1",
    "typescript": "~3.1",
    "uglifyjs-webpack-plugin": "~2.0",
    "webpack": "~4.29",
    "webpack-cli": "~3.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "~4.2",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-stats-plugin": "^0.3.1",
    "npm-run-all": "^4.1.5",
