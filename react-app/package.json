{
  "name": "we-retail-journal-react-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5678",
  "dependencies": {
    "@adobe/cq-react-editable-components": "^1.1.0",
    "@adobe/cq-spa-component-mapping": "^1.0.3",
    "@adobe/cq-spa-page-model-manager": "^1.1.1",
    "cross-fetch": "^3.0.4",
    "custom-event-polyfill": "^1.0.7",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-async-script": "^1.1.1",
    "react-dom": "^16.12.0",
    "react-open-weather": "^0.6.2",
    "react-router-dom": "^5.1.2",
    "react-universal-component": "^4.0.1",
    "url-polyfill": "^1.1.8"
  },
  "scripts": {
    "linter": "eslint --ext .js,.jsx .",
    "linter:fix": "eslint --ext .js,.jsx . --fix",
    "build": "cross-env NODE_ENV=development npm-run-all build-css build-js",
    "build:production": "cross-env NODE_ENV=production npm-run-all build-css build-js",
    "build-js": "webpack --config config/webpack.config.dev.js && webpack --config config/webpack.config.server.dev.js",
    "build-css": "node-sass-chokidar --source-map-embed --include-path ./src --include-path ./node_modules src/ -o src/ --output-style compressed",
    "watch-css": "node-sass-chokidar --source-map-embed --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive --output-style expanded",
    "start-css": "npm-run-all -s build-css watch-css",
    "test": "npm run build && cross-env NODE_ENV=test karma start --browsers=ChromeHeadless --single-run=true",
    "ci": "npm run test karma-ci.conf.js",
    "test:ci": "scripts/ci/test.sh",
    "test:debug": "npm run build && cross-env NODE_ENV=test karma start --browsers=Chrome_with_debugging",
    "start": "webpack-dev-server --env.dev",
    "start:server": "node --inspect src/server/bootstrap.js",
    "preversion": "npm run test && npm run build:production",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "start-aempack": "node scripts/aempack.js",
    "aempack": "cross-env PROXYHOST=local.we-retail-journal.com NODE_ENV=development npm-run-all -p watch-css start-aempack",
    "aempack-ssr": "cross-env DEVELOPSSR=true  NODE_ENV=development npm-run-all -p watch-css start-aempack",
    "aempack-pub": "cross-env PROXYHOST=local.we-retail-journal.com AEMPORT=4503 cross-env NODE_ENV=development npm-run-all -p watch-css start-aempack",
    "aempack-pub-ssr": "cross-env DEVELOPSSR=true npm-run-all -p aempack-pub"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "IE >= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "aempack": "^1.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-plugin-universal-import": "^4.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^9.1.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-flowtype": "^3.12.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "express": "^4.17.1",
    "file-loader": "^4.3.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "^8.1.0",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "^1.6.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react-dev-utils": "^9.1.0",
    "sass-loader": "7.1.0",
    "source-map-support": "^0.5.16",
    "style-loader": "^0.23.1",
    "style-to-object": "^0.2.3",
    "svg-inline-loader": "^0.8.0",
    "svg-inline-react": "^3.2.0",
    "terser-webpack-plugin": "^1.4.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-flush-chunks": "^2.0.3",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-stats-plugin": "^0.3.1",
    "workbox-webpack-plugin": "^4.3.1",
    "extra-watch-webpack-plugin": "^1.0.3"
  }
}
